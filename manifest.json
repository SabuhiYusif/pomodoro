{
  "name": "Pomodoro Extension",
  "version": "1.0",
  "description": "Pomodoro Timer",
  "options_page": "options.html",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "https://dog.ceo/api/breeds/image/random", "https://www.toggl.com/api/v8/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
          "16": "images/pomodoro_icon.png",
          "32": "images/pomodoro_icon.png",
          "48": "images/pomodoro_icon.png",
          "128": "images/pomodoro_icon.png"
        }
    },
  "icons": {
    "16": "images/pomodoro_icon.png",
    "32": "images/pomodoro_icon.png",
    "48": "images/pomodoro_icon.png",
    "128": "images/pomodoro_icon.png"
  },
  "manifest_version": 2
}
