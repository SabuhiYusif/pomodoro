{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "options_page": "options.html",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "http://aws.random.cat/meow", "https://dog.ceo/api/breeds/image/random", "https://www.toggl.com/api/v8/time_entries"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png",
          "128": "images/get_started128.png"
        }
    },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
